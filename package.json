    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-fetch": "^2.6.0",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.5.0",
    "style-loader": "^1.0.0",
    "tailwindcss": "^1.1.2",
